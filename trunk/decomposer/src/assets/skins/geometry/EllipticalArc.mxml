<ButtonSkin xmlns="assets.skins.*" xmlns:mx="http://www.adobe.com/2006/mxml"
	xmlns:geometry="com.degrafa.geometry.*" xmlns:paint="com.degrafa.paint.*">

	<extra_fills>
		<paint:SolidFill id="blue" color="#62ABCD"/>
	</extra_fills>

	<extra_strokes>
		<paint:SolidStroke id="white" color="#FFFFFF" alpha="1" weight="2"/>
	</extra_strokes>

	<extra_geometry>

		<geometry:EllipticalArc closureType="open"
			arc="180" width="40" height="40" x="0" y="5"
			startAngle="10" stroke="{white}" />

		<geometry:EllipticalArc closureType="chord"
			arc="90" width="30" height="40" x="5" y="10"
			startAngle="90" fill="{blue}" stroke="{white}"/>

		<geometry:EllipticalArc closureType="pie"
			arc="45" width="30" height="30" x="0" y="5"
			startAngle="300" fill="{blue}" stroke="{white}"/>
			
	</extra_geometry>

</ButtonSkin>