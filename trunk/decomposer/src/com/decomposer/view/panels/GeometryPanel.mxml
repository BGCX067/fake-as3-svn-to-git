	<mx:Button id="btn_horizontal_line" label="Horizontal Line"/>
	<mx:Button id="btn_vertical_line" label="Vertical Line"/>
	<mx:Button id="btn_line" label="Line"/>
	<mx:Button id="btn_polyline" label="Polyline"/>
-->


</containers:FlowBox>

</GeometryPanelCtrl>