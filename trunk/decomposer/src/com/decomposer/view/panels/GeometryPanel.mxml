<GeometryPanelCtrl xmlns="com.DeComposer.controller.panels.*"
	xmlns:mx="http://www.adobe.com/2006/mxml" xmlns:containers="flexlib.containers.*">

<containers:FlowBox width="100%" height="100%" backgroundColor="#333333" paddingLeft="5" paddingRight="5"
	paddingTop="5" paddingBottom="5">

	<mx:Button id="btn_circle" click="add('Circle')"
		 skin="assets.skins.geometry.Circle" height="40" width="40"
		 buttonMode="true" useHandCursor="true"/>

	<mx:Button id="btn_ellipse" click="add('Ellipse')"
		skin="assets.skins.geometry.Ellipse" height="40" width="40"
		 buttonMode="true" useHandCursor="true"/>

	<mx:Button id="btn_rectangle" click="add('Rectangle')"
		 skin="assets.skins.geometry.Rectangle" height="40" width="40"
		 buttonMode="true" useHandCursor="true"/>

	<mx:Button id="btn_rounded_rectangle" click="add('RoundedRectangle')"
		 skin="assets.skins.geometry.RoundedRectangle" height="40" width="40"
		 buttonMode="true" useHandCursor="true"/>

	<mx:Button id="btn_elliptic_arc" click="add('EllipticalArc')"
		 skin="assets.skins.geometry.EllipticalArc" height="40" width="40"
		 buttonMode="true" useHandCursor="true"/>

